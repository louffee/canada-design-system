{
  "name": "canada",
  "version": "0.1.0",
  "description": "Canada is the Louffee's Design System intended to make beautiful UIs with a lot of UX touches.",
  "repository": "git@github.com:louffee/canada-design-system.git",
  "author": "Louffee Dev. Team <team@louffee.dev>",
  "license": "MIT",
  "private": true,
  "homepage": "https://louffee.github.io/canada-design-system",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lint": "eslint --ext .ts,.tsx",
    "build": "yarn run clean && node ./scripts/build.js",
    "clean": "rimraf packages/**/build",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006 --ci",
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "sb": "yarn run storybook",
    "publish": "npx lerna publish",
    "bootstrap": "npx lerna bootstrap && npx lerna link",
    "commit": "./node_modules/.bin/cz",
    "ci": "yarn run commit",
    "test": "jest",
    "install:lint-dependencies": "node ./scripts/install-lint-related-dependencies.js",
    "prepare": "./node_modules/.bin/husky install",
    "lint:log": "yarn run lint > logs/$(date +'%Y-%m-%d_%H-%M-%S').log",
    "lint:auto-fix": "yarn run lint --fix",
    "test-docs": "./node_modules/.bin/alex",
    "test-docs:diff": "./node_modules/.bin/alex --diff",
    "deploy-storybook": "npx gh-pages -d storybook-static"
  },
