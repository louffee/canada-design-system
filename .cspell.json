{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/packages/cspell-types/cspell.schema.json",
  "ignorePaths": [
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/**",
    ".vscode",
    "megalinter",
    "package-lock.json",
    "report",
    "**.js"
  ],
  "patterns": [
    {
      "pattern": "packages/**.tsx",
      "name": "typescriptreact"
    },
    {
      "pattern": "packages/**.ts",
      "name": "typescript"
    }
  ],
  "language": "en",
  "noConfigSearch": true,
  "words": [
    "Autobuild",
    "Behaviour",
    "buildscript",
    "cacheable",
    "camelcase",
    "codeql",
    "commitlint",
    "docgen",
    "forin",
    "immed",
    "isnan",
    "latedef",
    "linebreak",
    "louffee",
    "markdownlint",
    "mrlemoos",
    "newcap",
    "noarg",
    "noempty",
    "nonbsp",
    "quotmark",
    "renovatebot",
    "SEMIBOLD",
    "stylelint",
    "stylelintrc"
  ],
  "version": "0.2"
}
